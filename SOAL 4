import numpy as np
import matplotlib.pyplot as plt

x_min = 0  
x_max = 30  

x = np.linspace(x_min, x_max, 1000)  

F = 2 * x**2 + 4 * x + 2

plt.figure(figsize=(10, 5))
plt.plot(x, F, color="b", linestyle="-", label=r"F(x) = 2x^2 + 4x + 2")
plt.xlabel("Posisi (meter)")
plt.ylabel("Gaya (Newton)")
plt.title("Grafik Gaya Tak Konstan terhadap Posisi")
plt.grid(True)
plt.legend()
plt.show()
